<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xmlns:camunda="http://camunda.org/schema/1.0/bpmn"
             targetNamespace="Examples">
  <process id="applicationProcess" name="Application Process" isExecutable="true" camunda:historyTimeToLive="180">
    <startEvent id="start" />
    <sequenceFlow id="s1" sourceRef="start" targetRef="svcKyc" />

    <serviceTask id="svcKyc" name="KYC Check" camunda:type="external" camunda:topic="kyc-check" />
    <sequenceFlow id="s2" sourceRef="svcKyc" targetRef="svcUw" />

    <serviceTask id="svcUw" name="Underwriting" camunda:type="external" camunda:topic="underwriting" />
    <sequenceFlow id="s3" sourceRef="svcUw" targetRef="svcPricing" />

    <serviceTask id="svcPricing" name="Pricing" camunda:type="external" camunda:topic="pricing" />
    <sequenceFlow id="s4" sourceRef="svcPricing" targetRef="end" />

    <endEvent id="end" />
  </process>
</definitions>
