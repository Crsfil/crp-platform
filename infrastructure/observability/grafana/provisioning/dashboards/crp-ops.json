{
  "id": null,
  "uid": "crp-ops",
  "title": "CRP Ops",
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "30s",
  "panels": [
    {
      "type": "stat",
      "title": "Services Up",
      "gridPos": { "h": 6, "w": 8, "x": 0, "y": 0 },
      "targets": [
        { "expr": "sum(up)", "refId": "A" }
      ],
      "options": {
        "reduceOptions": { "calcs": [ "lastNotNull" ] },
        "orientation": "auto"
      }
    },
    {
      "type": "timeseries",
      "title": "Request Rate (RPS)",
      "gridPos": { "h": 8, "w": 16, "x": 8, "y": 0 },
      "targets": [
        { "expr": "sum(rate(http_server_requests_seconds_count[5m]))", "refId": "A" }
      ]
    },
    {
      "type": "timeseries",
      "title": "5xx Rate",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 6 },
      "targets": [
        {
          "expr": "sum(rate(http_server_requests_seconds_count{status=~\"5..\"}[5m]))",
          "refId": "A"
        }
      ]
    }
  ],
  "annotations": { "list": [] },
  "templating": { "list": [] }
}
